<Window x:Class="WpfApp1.DashboardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="DashboardWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataframe" AutoGenerateColumns="True" Margin="0,0,0,76" SelectionMode="Single"/>
        <Button x:Name="DeleteBtn" Content="Delete" HorizontalAlignment="Left" Margin="193,386,0,0" VerticalAlignment="Top" Click="DeleteBtn_Click" Width="100" Height="25" Background="#FF0F0E0E" FontSize="16" Foreground="#FFF7F3F3"/>
        <Button x:Name="SignInBtn" Content="Logout" HorizontalAlignment="Left" Margin="482,386,0,0" VerticalAlignment="Top" Click="SignInBtn_Click" Width="97" Height="25" Background="#FF1A1919" FontSize="16" Foreground="#FFF7F3F3"/>

    </Grid>
</Window>
