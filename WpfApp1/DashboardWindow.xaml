<Window x:Class="WpfApp1.DashboardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="DashboardWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataframe" AutoGenerateColumns="True" Margin="0,0,0,76" SelectionMode="Single"/>
        <Button x:Name="DeleteBtn" Content="Button" HorizontalAlignment="Left" Margin="214,396,0,0" VerticalAlignment="Top" Click="DeleteBtn_Click"/>
        <Button x:Name="SignInBtn" Content="Button" HorizontalAlignment="Left" Margin="429,396,0,0" VerticalAlignment="Top" Click="SignInBtn_Click"/>

    </Grid>
</Window>
